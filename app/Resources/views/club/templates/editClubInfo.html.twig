<div id="club_contact" class="collapse">
	<div class="row">
		<div class="col-md-offset-4 col-md-1">
			{%trans%}Name{%endtrans%}
		</div>
		<div class="col-md-3 form-group input-group-md">
			{% if owner %} 
				{{form_widget(form.nombreContacto)}}
			{% else %}
				{{ club.nombreContacto}}&nbsp
			{% endif %}
				
		</div>
	</div>
	<div class="row">
		<div class="col-md-offset-4 col-md-1">
			{%trans%}Email{%endtrans%}
		</div>
		<div class="col-md-3 form-group input-group-md">
